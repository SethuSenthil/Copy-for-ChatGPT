{
    "manifest_version": 3,
    "author": "Sethu Senthil",
    "name": "Copy for Chat GPT",
    "icons": {
        "16": "media/16.png",
        "19": "media/19.png",
        "38": "media/38.png",
        "48": "media/48.png",
        "128": "media/128.png"
    },
    "description": "This extension allows you to copy chat GPT responses with a click of a button or CMD+K and runs it through plagiarism detectors",
    "version": "0.3.7",
    "host_permissions": [
        "https://api.gptzero.me/*"
    ],
	"permissions": [
		"storage"
	],
    "background": {
        "service_worker": "background.js"
    },
	"options_page": "options.html",
    "content_scripts": [
        {
            "js": [
                "init.js","settings.js"
            ],
            "css": [
                "styles.css"
            ],
            "matches": [
                "https://chat.openai.com/*"
            ]
        }
    ]
}