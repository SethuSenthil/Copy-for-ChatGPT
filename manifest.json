{
    "manifest_version": 3,
    "author": "Sethu Senthil",
    "name": "Copy for Chat GPT",
    "icons": {
        "16": "media/16.jpg",
        "19": "media/19.jpg",
        "38": "media/38.jpg",
        "48": "media/48.jpg",
        "128": "media/128.jpg"
    },
    "description": "This extension allows you to copy chat GPT responses with a click of a button or through CMD+K",
    "version": "0.1",
    "content_scripts":[
        {
            "js" : ["init.js"],
            "css": ["styles.css"],
            "matches" : ["https://chat.openai.com/*"]
        }
    ]
  }